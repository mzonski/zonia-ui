{
  "name": "@zonia-ui/components",
  "description": "UI Components with React & Typescript with Styled-Components",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "author": {
    "name": "Maciej Zonski",
    "email": "me@zonni.pl"
  },
  "license": "MIT",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*"
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "lint": "eslint components --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@zonia-ui/core": "*",
    "@zonia-ui/theme": "*",
    "eslint-plugin-zonia": "^1.0.1",
    "fp-ts": "^2.16.6",
    "lodash-es": "^4.17.21",
    "uuid": "^10.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.1.11",
    "utility-types": "^3.11.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/lodash": "^4.17.4",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.0",
    "@zonia-ui/storybook-config": "*",
    "eslint": "^8.45.0",
    "eslint-plugin-storybook": "^0.8.0",
    "vite": "^5.2.12",
    "vite-plugin-dts": "^3.9.1"
  }
}
